    "compression": "^1.5.2",
    "connect-livereload": "^0.5.3",
    "connect-mongo": "^0.8.1",
    "cookie-parser": "^1.3.5",
    "copy-webpack-plugin": "^4.5.1",
    "diff-array-objs": "~1.0.0",
    "ejs": "^2.3.3",
    "errorhandler": "^1.4.2",
    "express": "^4.13.3",
    "express-domain-middleware": "^0.1.0",
    "express-jwt": "^3.0.0",
    "express-session": "^1.11.3",
    "fs-extra": "~0.26.3",
